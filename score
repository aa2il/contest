#set CONTEST="iaru"
#set CONTEST="cols13"            # 13 colonies special event - worked in 2021
#set CONTEST="sats"              # Satellites worked  - worked in 2021

set d=`date`
if( X$CONTEST == "X" && ($d[1] == "Wed" || $d[1] == "Thu") )then
    set CONTEST="cwops"
else if( X$CONTEST == "X" )then
    set CONTEST="sst"
endif

################################################################################

cabrillo.py -$CONTEST

